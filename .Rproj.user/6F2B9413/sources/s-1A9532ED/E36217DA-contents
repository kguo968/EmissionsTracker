site_name_match_df <- bind_rows(pm25site, no2site, pm10site, so2site, cosite, ozonesite) %>%
  distinct(site, .keep_all = TRUE) %>%
  dplyr::rename(site_old = site) %>%
  dplyr::select(site_old, site_name)

